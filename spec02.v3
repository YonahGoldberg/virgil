class Animal(name: string) {
    def speak() -> string {
        return "boop";
    }
    def animalSpeak() -> string {
        return "beep";
    }
}
class Cow extends Animal {
    new(name: string) super(name) {}
    def speak() -> string {
        return "moo";
    }
}

def accept(a: Animal) -> string {
    return a.animalSpeak();
}

def main() -> string {
    var cow = Cow.new("cow");
    // var cat = Animal.new("cat");
    return accept(cow);
}